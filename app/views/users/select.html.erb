<h1 class="test">User details</h1>

<div class="test">
Name: <%= @user.name %>
<br>
Email: <%= @user.email %>
<br>
City:
  <ul>
    <% @user.cities.each do |city| %>
      <li> <%= city.name %></li>
    <% end %>
  </ul>

  Your Fav Cities:
  <ul>
    <% @user.fav_cities.each do |name| %>
      <li> <%= name.city_name %></li>
    <% end %>
  </ul>
</div>